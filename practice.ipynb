    "import heapq\r\n",
    "from heapq import heappush\r\n",
    "\r\n",
    "def heapsort(iterable):\r\n",
    "    h = []\r\n",
    "    for value in iterable:\r\n",
    "        heappush(h, value)\r\n",
    "    return h"
